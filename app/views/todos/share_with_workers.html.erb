<div class="hgt_auto">
  <div class="header_top_row">
    <span class="back_btn"><a href="/">&nbsp;</a></span>
    <span class="menu_icons">
      <div class="submenu_toggle">
        <ul>
          <li><a href="/">Home</a></li>
          <%if current_user%>
            <li><%=link_to 'Logout', destroy_user_session_path, method: :delete%></li>
          <%end%>
        </ul>
      </div>
    </span>
  </div>
</div>
<div class="mid_wrapp padd34">
  <%=form_tag share_with_workers_todo_path(@todo), method: :post do%>
    <div class="sel_col">
      <a class="select_txt" href="#">Select All</a>
    </div>
    <div class="custom_checbox_list">
      <ul>
        <%current_daycare.workers.each do |worker|%>
          <li>
            <%= check_box_tag 'worker_ids[]', worker.id, false, :id => "worker_id_#{worker.id}" %>
            <label for="worker_id_<%=worker.id%>"><%=worker.name%></label>
          </li>
        <%end%>
      </ul>
    </div>
    <div class="task_com_col">
      <h4>Once task are completed todo list delegatable</h4>
      <span class="yes_no">
      <a href="#" class="yn_active">Yes</a>
      <a href="#">No</a>
      </span>
    </div>
    <input name="" type="submit" class="submit_btn" value="Submit">
  <%end%>
</div>