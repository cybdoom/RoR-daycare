<div class="mid_wrapp padd34">
  <div class="cong_col">
    <h4>Congratulations</h4>
    <p>Invite to this applicaton Daycare Parents</p>
    <%=form_tag invite_parents_daycare_path(@daycare), method: :post do%>
      <div class="cong_input_wrap mr_up">
        <ol>
          <% for i in 1..3 %>
            <li><%= email_field_tag "email_ids[]", nil, placeholder: 'Daycare parent email', required: true %></li>
          <% end %>
        </ol>
      </div>
      <a class="add_files add_more_email_worker" id="add_more_parent_email" href="javascript:;">Add more...</a>
      <div class="btns_col">
        <%=submit_tag 'Send Invitation', class: 'submit_btn send_btn'%>
      </div>
    <%end%>
  </div>               
</div>