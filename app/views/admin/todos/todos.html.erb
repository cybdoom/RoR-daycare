          <h5>Create</h5>
        <%end%>
      </li>
      <li>
        <%=link_to search_admin_todos_path do%>
          <%=image_tag 'edi_icon.png', width: '77', height: '64'%>
          <h5>Edit</h5>
        <%end%>
        </li>
      <li>
        <%=link_to "#" do%>
          <%=image_tag 'report_icon.png', width: '77', height: '64'%>
          <h5>Report</h5>
        <%end%>
      </li>
    </ul>
  </div>
</div>	