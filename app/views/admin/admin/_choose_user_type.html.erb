      </li>
      <li>
        <input type="checkbox" class="user_type_common" name="user_type[]" id="parents_type" value="Parent" />
        <label for="parents_type">Daycare Parents</label>
      </li>
      <li>
        <input type="checkbox" class="user_type_common" name="user_type[]" id="partner_type" value="Partner" />
        <label for="partner_type">Daycare Partner</label>
      </li>
    </ul>
  </div>
</div>
<script type="text/javascript">
  $('.select_user_type').click(function(){
    $('.user_type_common').click();
  });
  $('.user_type_common').click(function(){
    $('#user_type_').val($(this).val());
  });
</script>